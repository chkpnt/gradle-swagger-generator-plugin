{
  "extends": [
    "config:base"
  ],
  "labels": [
    "r:{{depName}}",
    "r:{{depName}}/{{newVersion}}"
  ],
  "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch", "pin"],
      "automerge": true
    }
  ],
  "regexManagers": [
    {
      "fileMatch": [
        "^README\.md$",
      ],
      "matchStrings": [
        "id 'org.hidetake.swagger.generator' version '(?<currentValue>.*?)'",
      ],
      "depNameTemplate": "int128/gradle-swagger-generator-plugin",
      "datasourceTemplate": "github-releases",
    },
  ],
}
